<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cht.springboot_mybaties.mapper.BookMapper" >
   <select id="getAllBooks" resultType="com.cht.springboot_mybaties.model.Book">
     select * from books
   </select>
   
   <select id='getBooks' resultType="com.cht.springboot_mybaties.model.Book"> 
      select * from books where id=1
   </select>
</mapper>